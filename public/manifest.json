{
  "manifest_version": 2,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",
  "icons": {
    "16": "logo192.png",
    "48": "logo512.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "content_security_policy": "script-src 'self' 'sha256-5N8NhP7EyiaJZC79ji7MacYHsNcijJvAL7NJSlHOIM4='; object-src 'self'",
  "permissions": ["https://*/*", "http://*/*", "tabs"]
}
